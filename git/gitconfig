[user]
	name  = <Your Name>
	email = <Your Email>

[core]
  excludesfile = ~/.gitignore

[color]
  branch    = auto
  diff      = true
  status    = true
[color "branch"]
  current   = yellow reverse
  local     = yellow
  remote    = green
[color "diff"]
  meta      = yellow bold
  frag      = magenta bold
  old       = red bold
  new       = green bold
[color "status"]
  added     = yellow
  changed   = green
  untracked = cyan

[alias]
  a         =      add
  aa        =      add .
  au        =      add -u .
  al        = !git config -l | sed -n 's/^alias\\.//p'
  b         =      branch
  c         =      commit -v
  ca        =      commit -v --all
  ci        =      commit -v --interactive
  co        =      checkout
  cb        =      checkout -b
  cf        =      config
  cl        =      config -l
  d         =      diff
  dc        =      diff --cached
  dd        = !git diff | less
  di        =      diff HEAD
  dp        =      diff HEAD^
  e         =      reset
  ee        =      reset --soft HEAD^
  f         =      fetch
  g         =      grep -E -n
  h         =      help
  l         = !git log --reverse -10 --pretty=tformat:\"%Cblue%h %Cgreen%an %Cred%ai %Creset- %s\"
  ll        =      log --reverse -10
  lp        = !git log --reverse -10 -p | less
  la        = !git log | less
  lv        = !git log -p | less
  lc        =      whatchanged --reverse -10
  lf        = !git whatchanged | less
  m         =      merge
  o         =      remote
  oo        = !git remote -v | grep @ | cut -f1
  p         =      push
  pa        = !git remote -v | grep @ | cut -f1 | xargs -I '{}' git push '{}' master
  s         =      status
  u         =      pull
  v         =      revert
  w         =      show --pretty=fuller
  x         =      commit -v -a --amend
  z         =      checkout-index -u -f
  zz        =      checkout -f
