# See http://github.com/blackwinter/brice for more info
begin
  require 'rubygems'
  require 'brice/init'
rescue LoadError
end

# Alternatively, put your extensions in ~/.irbrc.d
Dir["#{__FILE__}.d/*.rb"].sort.each { |rc| load rc }

### LOAD CUSTOM FILE
if File.readable?(mine = "#{__FILE__}.mine")
  load mine
end
